<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="codingM.dao.SongDao">
  <resultMap type="Song" id="SongMap">
  </resultMap>
  
  <insert id="insert" parameterType="Song">
    insert into sing(sno, mno, title, singer, recordDate, fileName) values(#{sno}, #{mno}, #{title}, #{singer}, #{recordDate}, #{fileName}); 
  </insert>

  <update id="update" parameterType=""></update>
  
  <delete id="delete" parameterType="int">
    delete from sing where mno=#{mno};
  </delete>
  
  <select id="selectList" resultMap="SongMap">
    select * from sing
  </select>
  
  <select id="selectOne" parameterType="int" resultMap="SongMap">
    select * from sing where mno=#{mno}
  </select>
  
</mapper>












